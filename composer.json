{
  "name": "php-yaoi/php-yaoi",
  "type": "library",
  "description": "Yaoi is yet another object interoperator",
  "keywords": ["framework"],
  "homepage": "http://php-yaoi.tk/",
  "license": "MIT",
  "authors": [
    {
      "name": "Viacheslav Poturaev",
      "email": "vearutop@gmail.com",
      "homepage": "https://github.com/vearutop"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "monolog/monolog": "*",
    "doctrine/annotations": "^1.2"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "dev-master",
    "phpunit/phpunit": "~4.0",
    "psr/log": "~1.0",
    "phpdocumentor/phpdocumentor": "^2.8",
    "evert/phpdoc-md": "^0.1.1"
  },
  "autoload": {
    "psr-4": {
      "Yaoi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yaoi\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
